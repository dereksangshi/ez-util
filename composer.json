{
    "name": "ez/util",
    "type": "library",
    "description": "Util",
    "keywords": ["util", "data", "container"],
    "homepage": "https://bitbucket.org/dereksangshi2000/ez-util",
    "license": "MIT",
    "authors": [
        {"name": "Derek Li", "email": "dereksangshi2000@gmail.com.au"}
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*",
        "symfony/console": "~2.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://satis.ez.com"
        }
    ],
    "suggest": {
        "symfony/console": "For helpful console commands to test."
    },
    "autoload": {
        "psr-0": { "Ez\\Util\\": "src/" }
    },
    "archive": {
        "exclude": ["!vendor", "tests", "*phpunit.xml", ".travis.yml", "butilld.xml", "butilld.properties", "composer.phar"]
    }
}
